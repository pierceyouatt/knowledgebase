<h1>
  Add a new project type
</h1>


<hr>

<!-- Form project_type start -->
<form action="/create_project_type" method="post">
<!-- Form project_type end -->

  <!-- Label and input for type_name start -->
  <div class="form-group">
    <label for="type_name">
      Type name
    </label>

    <!-- Input for type_name start -->
    <input type="text" id="type_name" name="type_name" class="form-control">
    <!-- Input for type_name end -->
  </div>
  <!-- Label and input for type_name end -->

  <!-- Label and input for sol_id start -->
  <div class="form-group">
    <label for="sol_id">
      Sol
    </label>

    <!-- Input for sol_id start -->
    <input type="text" id="sol_id" name="sol_id" class="form-control">
    <!-- Input for sol_id end -->
  </div>
  <!-- Label and input for sol_id end -->

  <button class="btn btn-block btn-outline-secondary">
    Create project type
  </button>
</form>

<hr>

<a href="/project_types">
  Go back
</a>
